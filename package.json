{
	"name": "PowerTerminal",
	"displayName": "Power Terminal",
	"description": "Create your custtom Commands and run them with one in Terminal",
	"version": "1.0.0",
	"publisher": "Ahmed_Elsayeh",
	"author": {
		"name": "sayehxp",
		"email": "sayehxp@gmail.com"
	},
  
  "engines": {
    "vscode": "^1.0.0"
  },
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:extension.createnew",
    "onCommand:extension.execute",
    "onCommand:extension.catcharg",
    "onCommand:extension.sync"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.sync",
        "title": "Sync All",
        "category": "Power Terminal"
      },
      {
        "command": "extension.createnew",
        "title": "Create Command",
        "category": "Power Terminal"
      },
      {
        "command": "extension.execute",
        "title": "Run Command",
        "category": "Power Terminal"
      },
      {
        "command": "extension.catcharg"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+shift+alt+99",
        "command": "extension.sync"
      },
      {
        "key": "ctrl+shift+alt+1",
        "command": "extension.catcharg",
        "args": "1"
      },
      {
        "key": "ctrl+shift+alt+2",
        "command": "extension.catcharg",
        "args": "2"
      },
      {
